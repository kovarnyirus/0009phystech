mixin s-offices-14()
	// start s-offices
	+b.s-offices.section.bgn#s-offices&attributes(attributes)
		.container
			+b.section-title
				h2 Отдельные офисы
			+e.row.row
				mixin item(i, t,  t2, li1, li2, pr1, pr2, pr3 )
					+e.col.col-xl-4.col-md-6
						+e.item&attributes(attributes)
							+e.img-wrap.img-bg(href="#" style=`background-image: url(img/office-${i}.jpg);`)
								img(src=`img/office-${i}.jpg`, alt="")
							+e.caption
								.row
									.col
										+e.title= t
									.col-auto.pt-2
										+icon('user')
										|
										strong= t2
								ul
									li= li1
									li= li2

								.row.align-items-center.row--sm
									.col
										table
											tr
												td.small Первый час:
												td.h5.strong.text-primary= pr1

											tr
												td.small Последующие:
												td.h5.strong.text-primary= pr2

											tr
												td.small День:
												td.h5.strong.text-primary= pr3
