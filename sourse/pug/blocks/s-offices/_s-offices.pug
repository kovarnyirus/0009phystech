mixin s-offices()
	// start s-offices
	+b.s-offices.section#s-offices&attributes(attributes)
		.container
			+b.section-title
				h2 Отдельные офисы

			+e.row.row
				mixin item(i, t, pr)
					+e.col.col-xl-4.col-md-6
						+e.item&attributes(attributes)
							+e.img-wrap.img-bg(href="#" style=`background-image: url(img/office-${i}.jpg);`)
								img(src=`img/office-${i}.jpg`, alt="")
							+e.caption
								+e.title= t
								+e.item-row.row.h4.row--sm
									.col-auto
										+icon('user')
										|
										strong 10

									.col-auto
										// 08.02 добавил
										+icon('square')
										//- span.text-primary S
										|
										strong 80 кв.м
									.col.text-right.strong 7 этаж

								.row.align-items-center.row--sm
									.col
										+e.price= pr
									.col-auto
										+e.btn(href="#") подробнее
					// +e.col.col-xl-4.col-md-6
				+item('1','Офис Премьер','50 000 ₽/м')
				+item('2','Офис Эконом','20 000 ₽/м')
				+item('3','Офис Президент','150 000 ₽/мес.')
	// end s-offices

